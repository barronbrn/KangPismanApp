<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="32dp"
        android:height="32dp">
        <shape android:shape="oval">
            <solid android:color="#400288D1" /> </shape>
    </item>
    <item
        android:width="16dp"
        android:height="16dp"
