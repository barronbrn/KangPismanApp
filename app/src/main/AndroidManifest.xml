        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo_kangpisman"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_kangpisman"
        android:supportsRtl="true"
        android:theme="@style/Theme.KangPismanApp"
        tools:targetApi="31">
        <activity
            android:name=".view.activity.EditProfileActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.DetailArtikelActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.EdukasiActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.DaftarHargaActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.DetailTransaksiActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.BuatSetoranActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.MyQrCodeActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.PilihPeranActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.ScannerActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.TimbangActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.KangPismanApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.MainActivity"
            android:exported="false" />
    </application>

</manifest>